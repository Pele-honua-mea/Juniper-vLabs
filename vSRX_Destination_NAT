jcluser@vSRX1> show configuration security nat  
destination {
    pool DMZ_SERVER1_PL {
        address 10.100.11.2/32;
    }
    pool DMZ_SERVER2_PL {
        address 10.100.12.2/32;
    }
    rule-set DMZ_SERVER_RS {
        from zone untrust;
        rule DMZ_SERVER1_RL {
            match {
                destination-address 1.1.1.1/32;
            }
            then {
                destination-nat {
                    pool {
                        DMZ_SERVER1_PL;
                    }
                }
            }
        }
        rule DMZ_SERVER2_RL {
            match {
                destination-address 2.2.2.2/32;
            }
            then {
                destination-nat {
                    pool {
                        DMZ_SERVER2_PL; 
                    }                   
                }                       
            }                           
        }                               
    }                                   
}                                       
         
